<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/navFragment1">

    <fragment
        android:id="@+id/navFragment1"
        android:name="com.example.coroutinesdemo.coroutines.maps.demomaps.codelabmap.maps.navigation.bottomnavigation.BottomNavigationFragment1"
        android:label="fragment_signup"
        tools:layout="@layout/fragment_bottom_navigation1">


        <action
            android:id="@+id/action_navFragment1_to_navFragment3"
            app:destination="@id/navFragment3" />
        <action
            android:id="@+id/action_navFragment1_to_navFragment2"
            app:destination="@id/navFragment2" />
    </fragment>
    <fragment
        android:id="@+id/navFragment2"
        android:name="com.example.coroutinesdemo.coroutines.maps.demomaps.codelabmap.maps.navigation.bottomnavigation.BottomNavigationFragment1"
        android:label="fragment_signup"
        tools:layout="@layout/fragment_bottom_navigation2">


        <action
            android:id="@+id/action_navFragment2_to_navFragment1"
            app:destination="@id/navFragment1" />
        <action
            android:id="@+id/action_navFragment2_to_navFragment3"
            app:destination="@id/navFragment3" />
    </fragment>
    <fragment
        android:id="@+id/navFragment3"
        android:name="com.example.coroutinesdemo.coroutines.maps.demomaps.codelabmap.maps.navigation.bottomnavigation.BottomNavigationFragment1"
        android:label="fragment_signup"
        tools:layout="@layout/fragment_bottom_navigation3">


        <action
            android:id="@+id/action_navFragment3_to_navFragment1"
            app:destination="@id/navFragment1" />
    </fragment>


</navigation>