<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coroutinesdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoroutinesDemo">


        <service
            android:name=".coroutines.myservices.MusicService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".coroutines.CoroutineRetrofit" />
        <activity android:name=".coroutines.MainActivity" />
        <activity android:name=".coroutines.RunBlocking"/>
        <activity android:name=".coroutines.myservices.MusicActivity">

        <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>